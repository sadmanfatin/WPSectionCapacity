  <SQLQuery>
    <![CDATA[select * from (

 select  d.wp_section_id, 
 d.unit_section_id, 
 d.section_name,
 d.org_id,
 apps.org_id_to_org_name(d.org_id) org_name,
 d.day_no,
 trim(  to_char   ( day_val, 'Month')) month_name,
 extract(year from day_val) year,
-- D.CAPACITY,
 d.capacity_calc

 
from APPS.WP_DAY_WISE_SECTION_CAPACITY_V d

)

PIVOT(    min( capacity_calc )  

for day_no in (1 D1 ,2 D2 ,3 D3 ,4 D4 ,5 D5 ,6 D6 ,7 D7,8 D8, 9  D9,10 D10 ,
                11 D11 ,12 D12,13 D13 ,14 D14 ,15 D15,16 D16,17 D17 ,18 D18,19 D19,20 D20,
                21 D21 ,22 D22,23 D23,24 D24 ,25 D25,26 D26,27 D27 ,28 D28,29 D29,30 D30,31 D31) 

)]]>
  </SQLQuery>
